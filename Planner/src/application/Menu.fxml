<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <center>
      <AnchorPane blendMode="SRC_ATOP" prefHeight="700.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="create" layoutX="122.0" layoutY="107.0" mnemonicParsing="false" onAction="#goCreate" prefHeight="184.0" prefWidth="182.0" text="Create Account" />
            <Button layoutX="423.0" layoutY="372.0" mnemonicParsing="false" onAction="#goPack" prefHeight="184.0" prefWidth="182.0" text="View Packages" />
            <Button fx:id="createQuote" layoutX="122.0" layoutY="372.0" mnemonicParsing="false" onAction="#goQuote" prefHeight="184.0" prefWidth="182.0" text="Create Quote" />
            <Button fx:id="EditAcc" layoutX="423.0" layoutY="115.0" mnemonicParsing="false" prefHeight="184.0" prefWidth="182.0" text="Edit Account" />
            <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="678.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
